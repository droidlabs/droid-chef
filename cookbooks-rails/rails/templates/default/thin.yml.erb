pid: tmp/pids/thin.pid
log: log/thin.log
timeout: 30
max_conns: 1024
socket: /data/<%= @app_name %>/shared/tmp/pids/thin.sock
max_persistent_conns: 512
chdir: /data/<%= @app_name %>/current/<%= @thin_folder %>
environment: <%= @app_env %>
servers: <%= @servers_count %>
address: 0.0.0.0
daemonize: true