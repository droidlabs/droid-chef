# <%= @app_name %> - <%= @web_urls %>
upstream proxy_websocket_<%= @app_name %> {
  server 127.0.0.1:<%= @using_port + 3 %>;
  server 127.0.0.1:<%= @using_port + 4 %>;
  server 127.0.0.1:<%= @using_port + 5 %>;

  check interval=3000 rise=2 fall=5 timeout=1000;
}
server {
  listen 9020;
  server_name <%= @web_urls %>;

  tcp_nodelay on;
  websocket_pass proxy_websocket_<%= @app_name %>;
}